{
  "name": "agentd-secrets",
  "version": "0.1.0",
  "description": "Secret access broker with OIDC headless approval (Duo MFA) and Vault response wrapping",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "test": "jest --forceExit --detectOpenHandles",
    "test:unit": "jest --testPathPattern='tests/unit' --forceExit",
    "test:integration": "jest --testPathPattern='tests/integration' --forceExit",
    "test:e2e": "jest --testPathPattern='tests/e2e' --forceExit",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'"
  },
  "dependencies": {
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "jose": "^5.9.0",
    "js-yaml": "^4.1.0",
    "playwright": "^1.48.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.7.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nock": "^13.5.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
